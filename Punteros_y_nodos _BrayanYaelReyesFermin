#include <stdio.h>
#include <stdlib.h>

typedef struct Nodo {
    int dato;
    struct Nodo* siguiente;
    struct Nodo* anterior;
} Nodo;

Nodo* crearNodo(int valor) {
    Nodo* nuevo = (Nodo*)malloc(sizeof(Nodo));
    nuevo->dato = valor;
    nuevo->siguiente = NULL;
    nuevo->anterior = NULL;
    return nuevo;
}

void insertarFinal(Nodo** inicio, int valor) {
    Nodo* nuevo = crearNodo(valor);

    if (*inicio == NULL) {
        *inicio = nuevo;
        return;
    }

    Nodo* temporal = *inicio;
    while (temporal->siguiente != NULL) {
        temporal = temporal->siguiente;
    }

    temporal->siguiente = nuevo;
    nuevo->anterior = temporal;
}

void imprimirAdelante(Nodo* inicio) {
    Nodo* temporal = inicio;
    while (temporal != NULL) {
        printf("%d ", temporal->dato);
        temporal = temporal->siguiente;
    }
    printf("\n");
}

void imprimirAtras(Nodo* inicio) {
    Nodo* temporal = inicio;
    while (temporal && temporal->siguiente != NULL) {
        temporal = temporal->siguiente;
    }

    while (temporal != NULL) {
        printf("%d ", temporal->dato);
        temporal = temporal->anterior;
    }
    printf("\n");
}

int main() {
    Nodo* inicio = NULL;

    insertarFinal(&inicio, 10);
    insertarFinal(&inicio, 20);
    insertarFinal(&inicio, 30);
    insertarFinal(&inicio, 40);

    imprimirAdelante(inicio);
    imprimirAtras(inicio);

    return 0;
}